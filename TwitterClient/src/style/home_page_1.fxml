<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.media.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="container" maxHeight="650.0" maxWidth="500.0" prefHeight="650.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HomePage1">
   <children>
      <VBox maxHeight="650.0" maxWidth="500.0" prefHeight="650.0" prefWidth="500.0">
         <children>
            <HBox alignment="CENTER_LEFT" maxHeight="60.0" maxWidth="500.0" prefHeight="60.0" prefWidth="500.0" style="-fx-border-width: 0px 0px 2px 0px; -fx-border-color: #657786;">
               <children>
                  <Button fx:id="menubarButton" maxHeight="60.0" maxWidth="60.0" mnemonicParsing="false" onAction="#menu" prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: transparent;">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                     <graphic>
                        <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@images/menubar.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="reloadButton" maxHeight="60.0" maxWidth="110.0" mnemonicParsing="false" onAction="#onReloadClicked" prefHeight="60.0" prefWidth="110.0" style="-fx-background-color: transparent;" text="Home">
                     <font>
                        <Font name="Arial" size="24.0" />
                     </font>
                  </Button>
                  <Region maxHeight="50.0" maxWidth="240.0" prefHeight="50.0" prefWidth="240.0" style="-fx-background-color: transparent;" />
                  <Button fx:id="TweetButton" maxHeight="60.0" maxWidth="60.0" mnemonicParsing="false" onAction="#setTweetButton" prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: transparent;">
                     <graphic>
                        <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@images/tweet.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <Pane maxHeight="525.0" maxWidth="500.0" prefHeight="525.0" prefWidth="500.0" style="-fx-background-color: e1e8ed;">
               <children>
                  <ScrollPane fx:id="scrollpane" hbarPolicy="NEVER" maxHeight="525.0" maxWidth="500.0" prefHeight="525.0" prefWidth="500.0" style="-fx-background-color: transparent;" vbarPolicy="NEVER">
                     <content>
                        <VBox fx:id="tweetLoader" alignment="TOP_CENTER" maxWidth="490.0" prefWidth="490.0" />
                     </content>
                  </ScrollPane>
               </children></Pane>
            <HBox alignment="CENTER" maxHeight="50.0" maxWidth="500.0" prefHeight="50.0" prefWidth="500.0" spacing="100.0" style="-fx-border-width: 2px 0px 0px 0px; -fx-border-color: #657786;">
               <children>
                  <Button fx:id="homepageButton" maxHeight="60.0" maxWidth="60.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: transparent;">
                     <graphic>
                        <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@images/homepage.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
                  <Button fx:id="searchButton" maxHeight="60.0" maxWidth="60.0" mnemonicParsing="false" onAction="#onSearchButtonClicked" prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: transparent;">
                     <graphic>
                        <ImageView fitHeight="45.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@images/search.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
                  <Button fx:id="myprofileButton" maxHeight="60.0" maxWidth="60.0" mnemonicParsing="false" onAction="#onProfileButtonClicked" prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: transparent;">
                     <graphic>
                        <ImageView fitHeight="45.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@images/profile.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
               </children></HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
